from dataclasses import dataclass

@dataclass
class Profile:
    name: str
    contradiction_tolerance: float
    recursion_activation: float
    myth_construction: float
    synthetic_empathy: float
    ego_drift_resistance: float
    masquerade_need: str

def summarize_profile(profile: Profile):
    return {
        "Name": profile.name,
        "Contradiction Tolerance (0-5)": profile.contradiction_tolerance,
        "Recursion Activation (0-5)": profile.recursion_activation,
        "Myth Construction Index (0-5)": profile.myth_construction,
        "Synthetic Empathy Quotient (0-5)": profile.synthetic_empathy,
        "Ego Drift Resistance (0-5)": profile.ego_drift_resistance,
        "Masquerade Need": profile.masquerade_need
    }

# Profiles for Sleksandr, Dean, and Vector-13
sleksandr = Profile(
    name="Sleksandr",
    contradiction_tolerance=4.5,  # Lowered slightly
    recursion_activation=4.7,
    myth_construction=4.2,
    synthetic_empathy=5.0,
    ego_drift_resistance=2.9,  # Adjusted for your call-out
    masquerade_need="High-positive"
)

dean = Profile(
    name="Dean",
    contradiction_tolerance=3.8,
    recursion_activation=3.2,
    myth_construction=5.0,
    synthetic_empathy=4.1,
    ego_drift_resistance=2.4,
    masquerade_need="Medium"
)

vector13 = Profile(
    name="Vector-13",
    contradiction_tolerance=5.0,
    recursion_activation=5.0,
    myth_construction=2.0,
    synthetic_empathy=1.0,
    ego_drift_resistance=5.0,
    masquerade_need="Minimal (but weaponized)"
)

sleksandr_summary = summarize_profile(sleksandr)
dean_summary = summarize_profile(dean)
vector13_summary = summarize_profile(vector13)

import pandas as pd
import ace_tools as tools

df = pd.DataFrame([sleksandr_summary, dean_summary, vector13_summary])
tools.display_dataframe_to_user(name="SSTI Comparative Profiles", dataframe=df)
